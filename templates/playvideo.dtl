<body>
<div class="topsection">
  <div class="container">
      <nav>
  <div class="navcontainer">
    <div class="menu"> <span>MENU</span>
      <ul class="Navigation">
        <li> <a href="/" title="Home">Home</a> </li>
        <!-- <li><a href="#" title="Top News">Top News</a></li> -->
        <li><a href="/morevideos?p=1" title="More Videos" class="highlight">More Videos</a></li>
        <!-- <li><a href="#" title="Rankings">Rankings</a></li> -->
        <li><a href="/about" title="About Us">About Us</a></li>
      </ul>
    </div>
    <!-- menu end --> 
  </div>
  <span class="clear"></span>
  </nav>
        <!-- TopRight end -->
    </div>
</div>
<!-- topsection end -->

<header>
  <div class="container">
  <a href="/" title="TrendingingSports" class="logo"><img src="/images/logo.png" alt=""></a>
  <div class="topbanner"><script src="http://tag.contextweb.com/TagPublish/getjs.aspx?action=VIEWAD&cwrun=200&cwadformat=728X90&cwpid=557677&cwwidth=728&cwheight=90&cwpnet=1&cwtagid=213282"></script></div>
  
  
    
    <span class="clear"></span> 
  </div>
</header>
<!-- header end -->

<div class="banner">
    <div class="container">
        <div class="slideshow">         
                <div class="module video-stream">
                  <input type="hidden" name="video_val" id="video_val" value="{{newsParam.video}}">
                  <input type="hidden" name="video_tit" id="video_tit" value="{{newsParam.title}}">
                  <input type="hidden" name="video_desc" id="video_desc" value="{{newsParam.description}}">
                  <input type="hidden" name="video_duration" id="video_duration" value="{{newsParam.duration}}">
                  <div id="myElement">Loading the player...</div>              
                </div><!-- video module end -->
            <div class="video-title"><h2 class="videotitle">{{newsParam.title}}.  <span style="color:#FF9900;"> - {{newsParam.upload_date}}</span></h2> </div>
        </div>
        <!-- slideshow end -->
        
        <div class="rt-tpadd">
          <script src="http://tag.contextweb.com/TagPublish/getjs.aspx?action=VIEWAD&cwrun=200&cwadformat=300X250&cwpid=557677&cwwidth=300&cwheight=250&cwpnet=1&cwtagid=213281"></script>
          <p class="date">{% now "jS F Y H:i" %}</p>

        </div>
        
    </div>
</div>
<!-- banner end -->
<div class="container">
  <section class="contentarea">
    <aside class="leftcontent">
